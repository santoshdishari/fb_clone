<h1>Users</h1>
<%= link_to "create new user", new_user_path %>
<ul>
  <% @users.each do |user| %>
    <li>
      <%= user.first_name %>
    </li>
    <li>
      <%= user.last_name %>
    </li>
    <li>
      <%= user.email %>
    </li>
    <li>
      <%= user.phone %>
    </li>
     <li>
      <%= link_to user.first_name, user %>
    </li>
  <% end %>
</ul>